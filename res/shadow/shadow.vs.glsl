#version 330layout (location = 0) in vec3 position;layout (location = 1) in vec2 texCoord;layout (location = 2) in vec3 normals;varying vec4 ShadowCoord0;varying vec2 texCoord0;varying vec3 normals0;uniform mat4 MVP;uniform mat4 DepthBiasMVP;void main(){	gl_Position = MVP * vec4(position, 1.0);	ShadowCoord0 = DepthBiasMVP * vec4(position,1.0);	texCoord0 = texCoord;	normals0 = (MVP * vec4(normals,0.0)).xyz;}